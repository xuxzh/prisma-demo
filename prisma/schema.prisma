generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Operation {
  ip          String
  companyCode String
  companyDate String?
  user   String?
  customer    String?
  customerId  String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  uuid        String   @unique
  id          Int      @id @default(autoincrement())
}
